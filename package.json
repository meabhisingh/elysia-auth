{
  "name": "elysia-auth",
  "type": "module",
  "version": "0.0.0",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "description": "",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./providers/*": {
      "import": "./dist/providers/*.js",
      "types": "./dist/providers/*.d.ts"
    }
  },
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "prepublishOnly": "bun run build",
    "generate-providers": "bun ./scripts/generate-providers.ts",
    "clear": "rm -rf dist"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "bun"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/meabhisingh/elysia-auth#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/meabhisingh/elysia-auth.git"
  },
  "bugs": "https://github.com/meabhisingh/elysia-auth/issues",
  "author": "Abhishek Singh <official.6packprogrammer@gmail.com>",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/bun": "^1.1.10",
    "bun-plugin-dts": "^0.3.0",
    "fast-glob": "^3.3.3",
    "rollup": "^4.42.0",
    "rollup-plugin-terser": "^7.0.2",
    "tslib": "^2.8.1",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "elysia": "^1.3.4",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@auth/core": "^0.39.1",
    "@elysiajs/bearer": "^1.3.0"
  }
}
